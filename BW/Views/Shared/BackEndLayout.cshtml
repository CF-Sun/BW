<script>
    var isLogout = false;


    ////設定閒置後自動登出-----------------------------
    $(function () {
        $("body").on('click keypress', function () {
            ResetThisSession();
        });
    });
    var timeInSecondsAfterSessionOut = 1200; // change this to change session time out (in seconds). 1200
    var secondTick = 0;
    function ResetThisSession() {
        if (isLogout == false) {
            secondTick = 0;
            secondTick2 = 0;
            doCookieSetup("userRole", userRole);
            doCookieSetup("userAccount", userAccount);
        }
    }
    function StartThisSessionTimer() {
        secondTick++;
        var timeLeft = ((timeInSecondsAfterSessionOut - secondTick) / 60).toFixed(0); // in minutes
        timeLeft = timeInSecondsAfterSessionOut - secondTick; 

        if (secondTick > timeInSecondsAfterSessionOut) {

            $('#myModalForLogout').modal('show');
            clearTimeout(tick);
        }
        tick = setTimeout("StartThisSessionTimer()", 1000);
    }

    var timeInSecondsAfterSessionOut2 = 1381; // change this to change session time out (in seconds). 1380
    var secondTick2 = 0;
    function ResetThisSession2() {
        secondTick2 = 0;
    }
    function StartThisSessionTimer2() {
        secondTick2++;
        var timeLeft2 = ((timeInSecondsAfterSessionOut2 - secondTick2) / 60).toFixed(0); // in minutes
        timeLeft2 = timeInSecondsAfterSessionOut2 - secondTick2; 

        if (secondTick2 > timeInSecondsAfterSessionOut2) {
            Logout();
        }
        if (timeLeft2 >= 0)
            $('#lblsec').html(timeLeft2)
        tick2 = setTimeout("StartThisSessionTimer2()", 1000);
    }

    StartThisSessionTimer();
    StartThisSessionTimer2()
    //-----------------------------------------



    var userRole = readCookie('userRole');
    var userAccount = readCookie('userAccount');
    $(document).ready(function () {
        //檢查cookie 不存在則轉到登入頁
        if (userAccount == null || userAccount == '' || userAccount == undefined) {
            Logout()
            //window.location.href = "/BW/ConLogin";
        }  else if (userAccount == 'BW') {
            $('#accountName').html(userAccount + '您好')
            $('#li_1').show(); $('#li_2').show(); $('#li_3').show(); $('#li_4').show(); $('#li_5').show();
            $('#li_6').show(); $('#li_7').show(); $('#li_8').show(); $('#li_9').show(); $('#li_10').show();
            $('#li_11').show(); $('#li_12').show(); $('#li_13').show(); $('#li_14').show(); $('#li_15').show();
            $('#li_16').show(); $('#li_17').show(); /*$('#div_16').show(); $('#div_17').show();*/
        } else if (userRole == null || userRole == '' || userRole == undefined) {
            //alert("您帳號尚未設定權限,請聯絡管理員!")
            sweetAlert("您帳號尚未設定權限", "請聯絡管理員!", "warning");
            Logout()
            //window.location.href = "/BW/ConLogin";
        }
        else {
            $('#accountName').html(userAccount + '您好')
            var obj =
            {
                ROLE_ID: userRole
            }
            $.getJSON("/Account/GetRoleAuth", obj).done(function (data) {
                if (data[0].Auth_1 == "True")
                    $('#li_1').show();
                if (data[0].Auth_4 == "True")
                    $('#li_2').show();
                if (data[0].Auth_6 == "True")
                    $('#li_3').show();
                if (data[0].Auth_7 == "True") {
                    $('#li_4').show(); $('#li_7').show();
                }
                if (data[0].Auth_8 == "True") {
                    $('#li_5').show(); $('#li_6').show();
                }
                if (data[0].Auth_10 == "True")
                    $('#li_8').show();
                if (data[0].Auth_13 == "True") {
                    $('#li_9').show(); $('#li_10').show();
                }
                if (data[0].Auth_14 == "True")
                    $('#li_11').show();
                if (data[0].Auth_16 == "True")
                    $('#li_12').show();
                if (data[0].Auth_17 == "True") {
                    $('#li_13').show(); $('#li_14').show();
                }
                if (data[0].Auth_18 == "True")
                    $('#li_15').show();
                if (data[0].Auth_19 == "True") {
                        /*$('#div_16').show();*/ $('#li_16').show();
                }
            });
        }
    });
    function CancelLogout() {
        $('#myModalForLogout').modal('hide');
        doCookieSetup("userRole", userRole);
        doCookieSetup("userAccount", userAccount);

        ResetThisSession();
        ResetThisSession2();
        clearTimeout(tick);
        clearTimeout(tick2);
        StartThisSessionTimer();
        StartThisSessionTimer2()
    }
    function Logout() {
        isLogout = true;
        clearTimeout(tick);
        clearTimeout(tick2);
        delCookie("userRole");
        delCookie("userAccount");
        window.location.href = "/CH/ConLogin";
    }
    function EditEmail() {
        $('#myModalForEditEmail').modal('show'); 
    }
    function SubmitForEditEmail() {
        if ($('#txtAdmin_Email').val() == "") {
            //alert('請輸入Email');
            sweetAlert("請輸入Email!", "", "warning");
            return;
        }
        var obj = {};
