<script>
    var isLogout = false;
    var cliRole = readCookie('cliRole');
    var cliAccount = readCookie('cliAccount');

    ////設定閒置後自動登出-----------------------------
    $(function () {
        $("body").on('click keypress', function () {
            ResetThisSession();
        });
    });
    var timeInSecondsAfterSessionOut = 1200; // change this to change session time out (in seconds). 1200
    var secondTick = 0;
    function ResetThisSession() {
        if (isLogout == false) {
            secondTick = 0;
            secondTick2 = 0;
            doCookieSetup("cliRole", cliRole);
            doCookieSetup("cliAccount", cliAccount);
        }
    }
    function StartThisSessionTimer() {
        secondTick++;
        var timeLeft = ((timeInSecondsAfterSessionOut - secondTick) / 60).toFixed(0); // in minutes
        timeLeft = timeInSecondsAfterSessionOut - secondTick;

        if (secondTick > timeInSecondsAfterSessionOut) {

            $('#myModalForLogout').modal('show');
            clearTimeout(tick);
        }
        tick = setTimeout("StartThisSessionTimer()", 1000);
    }

    var timeInSecondsAfterSessionOut2 = 1381; // change this to change session time out (in seconds). 1380
    var secondTick2 = 0;
    function ResetThisSession2() {
        secondTick2 = 0;
    }
    function StartThisSessionTimer2() {
        secondTick2++;
        var timeLeft2 = ((timeInSecondsAfterSessionOut2 - secondTick2) / 60).toFixed(0); // in minutes
        timeLeft2 = timeInSecondsAfterSessionOut2 - secondTick2;

        if (secondTick2 > timeInSecondsAfterSessionOut2) {
            Logout();
        }
        if (timeLeft2 >= 0)
            $('#lblsec').html(timeLeft2)
        tick2 = setTimeout("StartThisSessionTimer2()", 1000);
    }

    StartThisSessionTimer();
    StartThisSessionTimer2()
    //-----------------------------------------

    $(document).ready(function () {
        //檢查cookie 不存在則轉到登入頁
        if (cliAccount == null || cliAccount == '' || cliAccount == undefined) {
            Logout()
        }
        else {
            $('#accountName').html(cliAccount + '您好')
        }
    });
    function CancelLogout() {
        $('#myModalForLogout').modal('hide');
        doCookieSetup("cliRole", cliRole);
        doCookieSetup("cliAccount", cliAccount);

        ResetThisSession();
        ResetThisSession2();
        clearTimeout(tick);
        clearTimeout(tick2);
        StartThisSessionTimer();
        StartThisSessionTimer2()
    }
    function Logout() {
        isLogout = true;
        delCookie("cliRole");
        delCookie("cliAccount");
        clearTimeout(tick);
        clearTimeout(tick2);

        window.location.href = "/CH/ConLogin";
    }
    function EditPW() {
        $('#txtOldPW').val('')
        $('#txtNewPW').val('')
        $('#txtNewPW2').val('')
        $('#myModalForEditPW').modal('show');
    }
    function SubmitForEditPW() {
        if ($('#txtOldPW').val() == "") {
            //alert('請輸入舊密碼');
            sweetAlert("請輸入舊密碼!", "", "warning");
            return;
        }
        if ($('#txtNewPW').val() == "") {
            //alert('請輸入新密碼');
            sweetAlert("請輸入新密碼!", "", "warning");
            return;
        }
        if ($('#txtNewPW').val() != $('#txtNewPW2').val()) {
            //alert('再次確認的新密碼不一致');
            sweetAlert("再次確認的新密碼不一致!", "", "warning");
            return;
        }
        var obj = {};
        obj.Account = cliAccount;
        obj.OldPW = $('#txtOldPW').val();
        obj.NewPW = $('#txtNewPW').val();
        $('#maindiv').jqLoading();
        var options = {};
        options.url = "/Account/cliAccountEditPW";
        options.type = "POST";
        options.data = JSON.stringify(obj);
        options.contentType = "application/json";
        options.dataType = "html";
        options.success = function (data) {
            if (data == 1) {
                //alert('修改成功!');
                swal("修改成功!", "", "success")
            } else if (data == 2) {
                //alert('修改失敗!舊密碼輸入錯誤');
                sweetAlert("修改失敗!", "舊密碼輸入錯誤", "error");
            }
            else {
                //alert('修改失敗!');
                sweetAlert("修改失敗!", "", "error");
            }
            $('#maindiv').jqLoading("destroy");
        };
        options.error = function () {
            //alert('修改失敗!');
            sweetAlert("修改失敗!", "", "error");
            $('#maindiv').jqLoading("destroy");
        };
        $.ajax(options);
    }
</script>
